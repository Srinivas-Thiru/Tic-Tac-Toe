ponents/Cell\"\n\n\n\nconst App = () => {\n\n  const [cells, setCells] = useState([\"\", \"\", \"\",\"\", \"\", \"\",\"\", \"\", \"\"]);\n  const [turn, setTurn] = useState(\"O\");\n  const [won, setWon] = useState(null);\n\n  const message = `${turn}'s turn!`\n\n\n  const checkScore = () =>{\n    const winningCombo = [\n      [0,1,2],\n      [3,4,5],\n      [6,7,8],\n      [0,3,6],\n      [1,4,7],\n      [2,5,8],\n      [0,4,8],\n      [2,4,6]\n    ]\n    winningCombo.forEach(combo => {\n      let OWins = combo.every(cell => cells[cell]===\"O\")\n      if(OWins) {\n        setWon(\"O Won!!!\")\n        document.querySelector(\"p\").classList.add(\"won\")\n        return\n      }\n\n      let XWins = combo.every(cell => cells[cell]===\"X\")\n      if(XWins) {\n        setWon(\"X Won!!!\")\n        document.querySelector(\"p\").classList.add(\"won\")\n\n        return\n      }\n    })\n\n  }\n\n\nuseEffect(() => {\n  checkScore()\n},[cells])\n\nconst handleReset = () => {\n  setTurn(\"O\")\n  setWon(null)\n  setCells([\"\",\"\",\"\",\"\",\"\",\"\",\"\",\"\",\"\"])  \n  document.querySelector(\"p\").classList.remove(\"won\")\n  document.getElementById(\"app\").classList.remove(\"reset\")\n  setTimeout(() => {\n    document.getElementById(\"app\").classList.add(\"reset\");\n  }, 10)\n\n}\n\n\n  return (\n    <div className=\"app reset\" id = \"app\">\n   <div className=\"gameboard\">\n      {cells.map((cell, index) => {\n        return <Cell \n        key={index}\n        id={index} \n        cell={cell}\n        cells={cells}\n        setCells={setCells}\n        turn = {turn}\n        setTurn = {setTurn}\n        won = {won} \n\n        />\n      })}\n\n   </div>\n   <p>{won || message}</p>\n\n   <button onClick={handleReset}> Reset </button>\n    </div>\n  )\n}\n\nexport default App\n","/Users/sri/Portfolio Projects/tic-tac-toe/src/components/Cell.js",[],[],"/Users/sri/Portfolio Projects/Tic-Tac-Toe/src/index.js",[],[],"/Users/sri/Portfolio Projects/Tic-Tac-Toe/src/